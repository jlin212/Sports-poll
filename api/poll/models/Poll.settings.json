{
  "connection": "default",
  "collectionName": "polls",
  "info": {
    "name": "poll",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "title": {
      "default": "",
      "type": "string",
      "required": true
    },
    "body": {
      "default": "",
      "type": "text"
    },
    "votes": {
      "default": 0,
      "type": "biginteger"
    },
    "title_bg": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "user": {
      "model": "user",
      "via": "polls",
      "plugin": "users-permissions"
    },
    "tags": {
      "collection": "tag",
      "via": "polls",
      "dominant": true
    },
    "opinions": {
      "collection": "opinion",
      "via": "poll"
    },
    "replies": {
      "default": "",
      "type": "integer"
    },
    "views": {
      "default": "",
      "type": "integer"
    },
    "league": {
      "model": "league",
      "via": "polls"
    },
    "date_start": {
      "default": "",
      "type": "date"
    },
    "date_end": {
      "default": "",
      "type": "date"
    },
    "upcoming_box": {
      "default": false,
      "type": "boolean"
    }
  }
}

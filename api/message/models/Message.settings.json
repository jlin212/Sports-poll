{
  "connection": "default",
  "collectionName": "messages",
  "info": {
    "name": "message",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "read": {
      "type": "boolean"
    },
    "message": {
      "default": "",
      "type": "json"
    },
    "name": {
      "default": "",
      "type": "string"
    },
    "users": {
      "collection": "user",
      "via": "messages",
      "dominant": true,
      "plugin": "users-permissions"
    },
    "from_user": {
      "model": "user",
      "via": "from_messages",
      "plugin": "users-permissions"
    },
    "to_user": {
      "model": "user",
      "via": "to_messages",
      "plugin": "users-permissions"
    }
  }
}
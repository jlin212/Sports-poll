{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "nickname": {
      "default": "",
      "type": "string"
    },
    "polls": {
      "collection": "poll",
      "via": "user"
    },
    "comments": {
      "collection": "comment",
      "via": "user"
    },
    "avatar": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "location": {
      "default": "",
      "type": "json"
    },
    "facebook": {
      "default": "",
      "type": "string"
    },
    "twitter": {
      "default": "",
      "type": "string"
    },
    "instagram": {
      "default": "",
      "type": "string"
    },
    "description": {
      "default": "",
      "type": "text"
    },
    "favorite_command": {
      "unique": true,
      "collection": "command",
      "via": "users_favorites",
      "dominant": true
    },
    "level": {
      "default": "",
      "type": "integer"
    },
    "following": {
      "default": {
        "ids": []
      },
      "type": "json"
    },
    "votes": {
      "default": {
        "ids": []
      },
      "type": "json"
    },
    "display_full_name": {
      "default": true,
      "type": "boolean"
    },
    "display_location": {
      "default": true,
      "type": "boolean"
    },
    "messages": {
      "collection": "message",
      "via": "users"
    },
    "opinions": {
      "collection": "opinion",
      "via": "user"
    }
  },
  "collectionName": "users-permissions_user"
}